{
	"id": "http://vcs.cisco.com/schemas",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"definitions": 
	{
		"id": "#/general/definitions",
		"IpAddressv4" : {
			"description" : "Ip adddress for Targetted message"
			, "type" : "String"
			, "pattern": "(([0-9]|[0-9][0-9]|[0-1][0-9][0-9]|2[0-4][0-9]|25[0-5]).){3}([0-9]|[0-9][0-9]|[0-1][0-9][0-9]|2[0-4][0-9]|25[0-5])"
		}
		, "MulticastIpAddress" : {
			"description" : "Ip adddress for Multicast message"
			, "type" : "String"
			, "pattern": "((22[4-9]|23[0-9]).)(([0-9]|[0-9][0-9]|[0-1][0-9][0-9]|2[0-4][0-9]|25[0-5]).){2}([0-9]|[0-9][0-9]|[0-1][0-9][0-9]|2[0-4][0-9]|25[0-5])"
		}
	}
}